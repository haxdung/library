<tr id = "<%= user.id %>">
  <td><%= user.name %></td>
  <td><%= user.email %></td>
  <td><%= user.role %></td>
  <td>
    <%= link_to edit_admin_user_url(user.id)   do %>
        <i class="fa fa-pencil fa-1x"></i>
    <% end %>
  </td>
  <td>
    <%= link_to user, method: :delete, data: {confirm: "Are you sure? "} do %>
        <i class="fa fa-trash fa-1x"></i>
    <% end %>
  </td>
</tr>
