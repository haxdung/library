<tr id = "<%= category.id %>">
  <td><%= category.name %></td>
  <td>
    <%= link_to edit_admin_category_url(category.id)   do %>
        <i class="fa fa-pencil fa-1x"></i>
    <% end %>
  </td>
  <td>
    <%= link_to category, method: :delete, data: {confirm: "Are you sure? "} do %>
        <i class="fa fa-trash fa-1x"></i>
    <% end %>
  </td>
</tr>
